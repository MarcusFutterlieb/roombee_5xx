<!--This is the roombee alphabet file that contains all the letters the robot can create via drive by odometry-->
<!--to add new symbols write down the length of each movement and the final oriantation (degree not radians) of the robot-->
<alphabet>

    <letter name ="A" out="bottom">
        <movement_sequence>
            <move_turn move="0.0" turn="45"/>
            <move_turn move="1.09" turn="-90"/>
            <move_turn move="0.4" turn="-135"/>
            <move_turn move="0.5" turn="180"/>
            <move_turn move="0.5" turn="-45"/>
            <move_turn move="0.609" turn="45"/>
        </movement_sequence>
    </letter>

    <letter name ="K" out="bottom">
        <movement_sequence>
            <move_turn move="0.0" turn="90"/>
            <move_turn move="1.0" turn="180"/>
            <move_turn move="0.5" turn="135"/>
            <move_turn move="0.6" turn="180"/>
            <move_turn move="0.6" turn="90"/>
            <move_turn move="0.6" turn="45"/>
        </movement_sequence>
    </letter>
    
    <letter name ="I" out="bottom">
        <movement_sequence>
            <move_turn move="0.0" turn="90"/>
            <move_turn move="0.3" turn="90"/>
            <move_turn move="1.0" turn="180"/>
            <move_turn move="1.0" turn="90"/>
        </movement_sequence>
    </letter>

    <letter name ="R" out="bottom">
        <movement_sequence>
            <move_turn move="0.0" turn="90"/>
            <move_turn move="1.0" turn="-90"/>
            <move_turn move="0.3" turn="-90"/>
            <move_turn move="0.3" turn="-90"/>
	    <move_turn move="0.3" turn="135"/>
            <move_turn move="0.76" turn="45"/>
        </movement_sequence>
    </letter>
    
    <letter name ="S" out="top">
        <movement_sequence>
            <move_turn move="0.5" turn="90"/>
            <move_turn move="0.5" turn="90"/>
            <move_turn move="0.5" turn="-90"/>
            <move_turn move="0.5" turn="-90"/>
            <move_turn move="0.5" turn="0"/>
        </movement_sequence>
    </letter>
    
    <letter name ="T" out="top">
        <movement_sequence>
            <move_turn move="0.3" turn="90"/>
            <move_turn move="1.0" turn="90"/>
            <move_turn move="0.3" turn="180"/>
            <move_turn move="0.6" turn="0"/>
        </movement_sequence>
    </letter>
    
    <letter name ="E" out="bottom">
        <movement_sequence>
            <move_turn move="0.0" turn="90"/>
            <move_turn move="1.0" turn="-90"/>
            <move_turn move="0.3" turn="180"/>
            <move_turn move="0.3" turn="90"/>
            <move_turn move="0.3" turn="90"/>
            <move_turn move="0.3" turn="180"/>
            <move_turn move="0.3" turn="90"/>
            <move_turn move="0.4" turn="90"/>
            <move_turn move="0.4" turn="180"/>
            <move_turn move="0.4" turn="0"/>
        </movement_sequence>
    </letter>

    <letter name ="M" out="bottom">
        <movement_sequence>
            <move_turn move="0.0" turn="90"/>
            <move_turn move="1.0" turn="-135"/>
            <move_turn move="1.09" turn="90"/>
            <move_turn move="1.09" turn="-135"/>
            <move_turn move="1.0" turn="90"/>
        </movement_sequence>
    </letter>
    
    <letter name ="N" out="top">
        <movement_sequence>
            <move_turn move="0.0" turn="90"/>
            <move_turn move="1.0" turn="-135"/>
            <move_turn move="1.09" turn="135"/>
            <move_turn move="1.0" turn="-135"/>
            <move_turn move="0.0" turn="-90"/>
        </movement_sequence>
    </letter>
    
</alphabet>
